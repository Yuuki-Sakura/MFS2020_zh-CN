      "Landing.Dlg_Intro8": {
        "UUID": "f6fe3299-80e9-4e5b-9442-5320c403a022",
        "Speaker": "INSTRUCTOR",
        "LastModifiedBy": "ajimeno",
        "LastModifiedDate": "2020-03-18 12:01:43",
        "LocalizationStatus": "TranslationNeeded",
        "Tags": {
          "Locked": "False"
        },
        "Languages": {
          "en-US": {
            "Text": "如果编号正在相对于你的挡风玻璃上移，那表示你的高度过低。",
            "LocalizationStatus": "TranslationNeeded"
          },
          "fr-FR": {
            "Text": "Si le numéro semble monter sur votre pare-brise, c'est que votre trajectoire est trop basse.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "es-ES": {
            "Text": "Si parece que el número se está moviendo hacia arriba en el parabrisas, es que vas bajo.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "de-DE": {
            "Text": "Wenn es scheint, als bewege sich die Nummer auf Ihrer Frontscheibe nach oben, sind Sie zu niedrig.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "it-IT": {
            "Text": "Se sembra che il numero salga lungo il parabrezza, stai andando troppo in basso...",
            "LocalizationStatus": "TranslationNeeded"
          },
          "pl-PL": {
            "Text": "Jeśli cyfry przesuwają się w górę przedniej szyby, to znaczy, że podchodzisz za nisko...",
            "LocalizationStatus": "TranslationNeeded"
          },
          "pt-BR": {
            "Text": "Se parecer que o número está se movendo para cima no para-brisas, significa que você está baixo.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "ru-RU": {
            "Text": "Если кажется, что номер движется вверх по лобовому стеклу, то вы заходите слишком низко…",
            "LocalizationStatus": "TranslationNeeded"
          },
          "zh-CN": {
            "Text": "如果编号正在相对于你的挡风玻璃上移，那表示你的高度过低。",
            "LocalizationStatus": "TranslationNeeded"
          }
        }
      },
