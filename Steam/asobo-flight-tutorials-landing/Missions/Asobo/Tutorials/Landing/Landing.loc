          }
        }
      },
      "Landing.Dlg_Intro8_MALE": {
        "UUID": "264ac95f-51ca-4527-890e-d3b3a5ea703e",
        "Speaker": "INSTRUCTOR",
        "LastModifiedBy": "ajimeno",
        "LastModifiedDate": "2020-03-18 12:01:43",
        "LocalizationStatus": "TranslationNeeded",
        "Languages": {
          "en-US": {
            "Text": "If the number appears to move up in your windscreen, you're coming in low..."
          },
          "fr-FR": {
            "Text": "Si le numéro semble monter sur votre pare-brise, c'est que votre trajectoire est trop basse.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "es-ES": {
            "Text": "Si parece que el número se mueve hacia arriba en el parabrisas, es que vas bajo.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "de-DE": {
            "Text": "Scheint sich die Nummer auf der Frontscheibe nach oben zu bewegen, sind Sie zu niedrig …",
            "LocalizationStatus": "TranslationNeeded"
          },
          "it-IT": {
            "Text": "Se il numero sale lungo il parabrezza, sei troppo in basso...",
            "LocalizationStatus": "TranslationNeeded"
          },
          "pl-PL": {
            "Text": "Jeśli cyfry przesuwają się w górę przedniej szyby, to znaczy, że podchodzisz za nisko...",
            "LocalizationStatus": "TranslationNeeded"
          },
          "pt-BR": {
            "Text": "Se parecer que o número está se movendo para cima no para-brisas, significa que você está baixo...",
            "LocalizationStatus": "TranslationNeeded"
          },
          "ru-RU": {
            "Text": "Если номер смещается вверх, значит, вы идёте слишком низко.",
            "LocalizationStatus": "TranslationNeeded"
          }
        }
      },
      "Landing.Dlg_MaintainSpeed1": {
        "UUID": "4cec0adf-2835-4ab8-8a7a-da45a3157530",
