      "Landing.Dlg_FlareNow1": {
        "UUID": "d70d74b3-cf84-4a10-a1d5-0c1d13206637",
        "Speaker": "INSTRUCTOR",
        "LastModifiedBy": "ajimeno",
        "LastModifiedDate": "2020-02-17 13:54:58",
        "LocalizationStatus": "TranslationNeeded",
        "Tags": {
          "Locked": "False"
        },
        "Languages": {
          "en-US": {
            "Text": "现在你可以开始拉平飞机了。",
            "LocalizationStatus": "TranslationNeeded"
          },
          "fr-FR": {
            "Text": "Vous êtes maintenant en mesure d'amorcer l'arrondi.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "es-ES": {
            "Text": "Ahora deberías ser capaz de empezar a enderezar el avión.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "de-DE": {
            "Text": "Jetzt sollten Sie in der Lage sein, Ihr Flugzeug ausschweben zu lassen.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "it-IT": {
            "Text": "Ora dovresti essere in grado di iniziare a richiamare il tuo aeromobile.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "pl-PL": {
            "Text": "Teraz chyba można rozpocząć wytrzymanie.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "pt-BR": {
            "Text": "Você deve ser capaz de iniciar o arredondamento do avião agora.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "ru-RU": {
            "Text": "Теперь можно начать выравнивание самолёта.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "zh-CN": {
            "Text": "现在你可以开始拉平飞机了。",
            "LocalizationStatus": "TranslationNeeded"
          }
        }
      },
      "Landing.Dlg_FlareNow1_MALE": {
