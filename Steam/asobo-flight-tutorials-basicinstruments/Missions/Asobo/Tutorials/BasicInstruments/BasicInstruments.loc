      "BasicInstruments.Warning_Too_High": {
        "UUID": "4c3f25fa-a3fc-483e-8314-360274a3ef5b",
        "LastModifiedBy": "ajimeno",
        "LastModifiedDate": "2020-03-18 12:01:41",
        "LocalizationStatus": "TranslationNeeded",
        "Tags": {
          "Locked": "False"
        },
        "Languages": {
          "en-US": {
            "Text": "飞机的位置有些偏高，下沉机头，减小油门，回到较低高度",
            "LocalizationStatus": "TranslationNeeded"
          },
          "fr-FR": {
            "Text": "Altitude trop élevée, tirer vers le bas et réduire les gaz pour perdre de l'altitude",
            "LocalizationStatus": "TranslationNeeded"
          },
          "es-ES": {
            "Text": "La altitud es demasiado elevada. Tira hacia abajo y reduce la aceleración para disminuir la altitud",
            "LocalizationStatus": "TranslationNeeded"
          },
          "de-DE": {
            "Text": "Sie sind zu hoch. Drücken Sie die Nase nach unten reduzieren Sie das Gas, um an Höhe zu verlieren.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "it-IT": {
            "Text": "Altitudine eccessiva, spingi verso il basso e riduci la manetta per tornare a un'altitudine inferiore.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "pl-PL": {
            "Text": "Zbyt duża wysokość, skieruj dziób samolotu w dół i przymknij przepustnicę, by zmniejszyć wysokość",
            "LocalizationStatus": "TranslationNeeded"
          },
          "pt-BR": {
            "Text": "Altitude muito alta, pique o manche e reduza o manete para voltar a uma altitude mais baixa",
            "LocalizationStatus": "TranslationNeeded"
          },
          "ru-RU": {
            "Text": "Вы слишком высоко — дайте штурвал от себя и сбросьте газ, чтобы снизиться",
            "LocalizationStatus": "TranslationNeeded"
          },
          "zh-CN": {
            "Text": "飞机的位置有些偏高，下沉机头，减小油门，回到较低高度",
            "LocalizationStatus": "TranslationNeeded"
          }
        }
      },
      "BasicInstruments.Warning_Too_Low": {
        "UUID": "c532e1c7-6e1f-44cb-9abf-ed29ebab5caa",
        "LastModifiedBy": "ajimeno",
        "LastModifiedDate": "2020-03-18 12:01:41",
        "LocalizationStatus": "TranslationNeeded",
        "Tags": {
          "Locked": "False"
        },
        "Languages": {
          "en-US": {
            "Text": "飞机高度过低，拉高机头，将油门开到最大，回到较高处",
            "LocalizationStatus": "TranslationNeeded"
          },
          "fr-FR": {
            "Text": "Altitude trop basse, tirer vers le haut et accélérer à plein régime pour reprendre de l'altitude",
            "LocalizationStatus": "TranslationNeeded"
          },
          "es-ES": {
            "Text": "La altitud es demasiado baja. Tira hacia arriba y acelera al máximo para recuperar altitud",
            "LocalizationStatus": "TranslationNeeded"
          },
          "de-DE": {
            "Text": "Sie sind zu niedrig, ziehen Sie hoch und geben Sie Vollgas, um an Höhe zu gewinnen",
            "LocalizationStatus": "TranslationNeeded"
          },
          "it-IT": {
            "Text": "Altitudine insufficiente, spingi e dai piena manetta per tornare a un'altitudine maggiore",
            "LocalizationStatus": "TranslationNeeded"
          },
          "pl-PL": {
            "Text": "Zaczynasz schodzić zbyt nisko: ściągnij wolant i całkowicie otwórz przepustnicę, by zwiększyć wysokość",
            "LocalizationStatus": "TranslationNeeded"
          },
          "pt-BR": {
            "Text": "Altitude muito baixa, cabre o manche e coloque o manete no máximo para elevar a altitude",
            "LocalizationStatus": "TranslationNeeded"
          },
          "ru-RU": {
            "Text": "Вы слишком низко — возьмите штурвал на себя и дайте полный газ, чтобы набрать высоту",
            "LocalizationStatus": "TranslationNeeded"
          },
          "zh-CN": {
            "Text": "飞机高度过低，拉高机头，将油门开到最大，回到较高处",
            "LocalizationStatus": "TranslationNeeded"
          }
        }
      }
