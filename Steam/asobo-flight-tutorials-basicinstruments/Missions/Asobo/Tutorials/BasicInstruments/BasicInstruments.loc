      "BasicInstruments.Dlg_LearnDescentAttitude1": {
        "UUID": "1f61786a-f7ef-4b9c-9173-32cb4aa813e9",
        "Speaker": "INSTRUCTOR",
        "LastModifiedBy": "ajimeno",
        "LastModifiedDate": "2020-02-17 13:54:57",
        "LocalizationStatus": "TranslationNeeded",
        "Tags": {
          "Locked": "False"
        },
        "Languages": {
          "en-US": {
            "Text": "如同预期，将机头下沉之后，我们的高度已经开始下降，飞机的速度也在提高。",
            "LocalizationStatus": "TranslationNeeded"
          },
          "fr-FR": {
            "Text": "Comme on s'y attendait, avec une assiette en piqué, notre altitude diminue alors que notre vitesse augmente. ",
            "LocalizationStatus": "TranslationNeeded"
          },
          "es-ES": {
            "Text": "Como era de esperar, con una actitud con el morro del avión hacia abajo, la altitud disminuye mientras que la velocidad aumenta.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "de-DE": {
            "Text": "Wie Sie sehen können, nimmt unsere Höhe ab und unsere Geschwindigkeit zu, wenn die Nase des Flugzeugs nach unten zeigt.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "it-IT": {
            "Text": "Come previsto, con un assetto discendente del muso, la nostra altitudine diminuisce, mentre la velocità aumenta.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "pl-PL": {
            "Text": "Jak można się było spodziewać, przy opuszczonym dziobie nasza wysokość spada, a prędkość rośnie. ",
            "LocalizationStatus": "TranslationNeeded"
          },
          "pt-BR": {
            "Text": "Como é de se esperar, com uma atitude de nariz para baixo, nossa altitude diminui enquanto nossa velocidade aumenta. ",
            "LocalizationStatus": "TranslationNeeded"
          },
          "ru-RU": {
            "Text": "Как и ожидалось, при пикировании наша высота уменьшается, а скорость растёт.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "zh-CN": {
            "Text": "如同预期，将机头下沉之后，我们的高度已经开始下降，飞机的速度也在提高。",
            "LocalizationStatus": "TranslationNeeded"
          }
        }
      },
      "BasicInstruments.Dlg_LearnDescentAttitude1_MALE": {
        "UUID": "29bbf479-7f0f-4e09-a92e-7719e93d2713",
        "Speaker": "INSTRUCTOR",
        "LastModifiedBy": "ajimeno",
        "LastModifiedDate": "2020-02-28 11:26:26",
        "Languages": {
          "en-US": {
            "Text": "With a nose-down Attitude, it's no surprise Altitude is decreasing and speed is on the rise."
          },
          "fr-FR": {
            "Text": "Avec une assiette en piqué, l'altitude diminue et la vitesse augmente, comme prévu."
          },
          "es-ES": {
            "Text": "Con una actitud con el morro hacia abajo, es normal que disminuya la altitud y aumente la velocidad."
          },
          "de-DE": {
            "Text": "Bei dieser Fluglage verlieren Sie natürlich an Höhe, und die Geschwindigkeit nimmt zu."
          },
          "it-IT": {
            "Text": "Come previsto, con un assetto discendente del muso, l'altitudine diminuisce e la velocità aumenta."
          },
          "pl-PL": {
            "Text": "Gdy dziób jest opuszczony, wysokość maleje, a prędkość rośnie."
          },
          "pt-BR": {
            "Text": "Com uma atitude de nariz para baixo, é de se esperar que a altitude diminua enquanto a velocidade aumenta."
          },
          "ru-RU": {
            "Text": "Когда нос направлен вниз, высота закономерно снижается, а скорость растёт."
          },
          "zh-CN": {
            "Text": "With a nose-down Attitude, it's no surprise Altitude is decreasing and speed is on the rise."
          }
        }
      },
      "BasicInstruments.Dlg_LearnDescentAttitude4": {
        "UUID": "d59fb12c-c212-4541-a745-f0f94ce31487",
        "Speaker": "INSTRUCTOR",
        "LastModifiedBy": "ajimeno",
        "LastModifiedDate": "2020-02-17 13:54:57",
        "LocalizationStatus": "TranslationNeeded",
        "Tags": {
          "Locked": "False"
        },
        "Languages": {
          "en-US": {
            "Text": "现在我们回到巡航姿态，进入本次课程的最后一部分。",
            "LocalizationStatus": "TranslationNeeded"
          },
          "fr-FR": {
            "Text": "Je vous laisse revenir à une assiette de croisière, et ensuite nous attaquerons la dernière partie de la leçon.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "es-ES": {
            "Text": "Volvamos a la actitud de crucero y terminemos la última parte de la lección.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "de-DE": {
            "Text": "Bevor wir uns dem letzten Teil der heutigen Flugstunde widmen, sollten Sie das Flugzeug zunächst zurück in die Reisefluglage bringen.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "it-IT": {
            "Text": "Perché non ci riporti a un assetto di crociera, così possiamo affrontare l'ultima parte della lezione.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "pl-PL": {
            "Text": "A może wróćmy do parametrów przelotowych i przejdźmy do ostatniej części naszej lekcji.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "pt-BR": {
            "Text": "Agora nos leve de volta a uma atitude de cruzeiro, e falaremos sobre a última parte da nossa lição.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "ru-RU": {
            "Text": "Давайте вернёмся в крейсерский режим и приступим к последней части нашего урока.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "zh-CN": {
            "Text": "现在我们回到巡航姿态，进入本次课程的最后一部分。",
            "LocalizationStatus": "TranslationNeeded"
          }
        }
      },
      "BasicInstruments.Dlg_LearnDescentAttitude4_MALE": {
