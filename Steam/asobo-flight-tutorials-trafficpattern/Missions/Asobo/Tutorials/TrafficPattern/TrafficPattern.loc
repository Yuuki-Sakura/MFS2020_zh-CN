      "TrafficPattern.Dlg_Downwind1": {
        "UUID": "5c4b2697-810f-4065-be04-e4d98d29ad2a",
        "Speaker": "INSTRUCTOR",
        "LastModifiedBy": "ajimeno",
        "LastModifiedDate": "2020-03-18 12:01:43",
        "LocalizationStatus": "TranslationNeeded",
        "Tags": {
          "Locked": "False"
        },
        "Languages": {
          "en-US": {
            "Text": "很好，操作完全正确！",
            "LocalizationStatus": "TranslationNeeded"
          },
          "fr-FR": {
            "Text": "Le vent est derrière nous. On est sur la bonne voie...",
            "LocalizationStatus": "TranslationNeeded"
          },
          "es-ES": {
            "Text": "Ahora tenemos el viento detrás, vamos en la dirección correcta...",
            "LocalizationStatus": "TranslationNeeded"
          },
          "de-DE": {
            "Text": "Jetzt haben wir Rückenwind. Sieht doch gut aus …",
            "LocalizationStatus": "TranslationNeeded"
          },
          "it-IT": {
            "Text": "Abbiamo il vento alle nostre spalle, siamo sulla rotta giusta...",
            "LocalizationStatus": "TranslationNeeded"
          },
          "pl-PL": {
            "Text": "Mamy teraz wiatr za plecami. Dobrze nam idzie…",
            "LocalizationStatus": "TranslationNeeded"
          },
          "pt-BR": {
            "Text": "O vento está nas nossas costas, agora. Estamos no caminho certo...",
            "LocalizationStatus": "TranslationNeeded"
          },
          "ru-RU": {
            "Text": "Ветер дует в спину. Отлично, мы на верном пути.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "zh-CN": {
            "Text": "很好，操作完全正确！",
            "LocalizationStatus": "TranslationNeeded"
          }
        }
      },
