      "TrafficPattern.Dlg_Downwind3_MALE": {
        "UUID": "46afa802-0519-4310-8f82-d9c44e87e554",
        "Speaker": "INSTRUCTOR",
        "LastModifiedBy": "ajimeno",
        "LastModifiedDate": "2020-02-28 11:26:27",
        "Languages": {
          "en-US": {
            "Text": "Once your speed is in the white arc, add 10 degrees of flaps and prepare for landing."
          },
          "fr-FR": {
            "Text": "Quand la vitesse est dans l'arc blanc, ouvrez les volets à 10 degrés pour vous préparer à l'atterrissage."
          },
          "es-ES": {
            "Text": "Cuando tu velocidad se halle en el arco blanco, incrementa 10 grados los flaps y prepárate para aterrizar."
          },
          "de-DE": {
            "Text": "Fahren Sie die Landeklappen um 10 Grad aus, sobald die Geschwindigkeit passt, und machen Sie sich zur Landung bereit."
          },
          "it-IT": {
            "Text": "Quando la velocità raggiunge l'area bianca, aggiungi 10° agli ipersostentatori e preparati all'atterraggio."
          },
          "pl-PL": {
            "Text": "Gdy strzałka prędkości będzie znajdować się na białym łuku, wypuść klapy na 10 stopni i przygotuj się do lądowania."
          },
          "pt-BR": {
            "Text": "Quando sua velocidade estiver no arco branco, adicione 10 graus de flaps e prepare-se para o pouso."
          },
          "ru-RU": {
            "Text": "Как только стрелка скорости попадёт в белую зону, выпустите закрылки ещё на 10 градусов и приготовьтесь к посадке."
          },
          "zh-CN": {
            "Text": "Once your speed is in the white arc, add 10 degrees of flaps and prepare for landing."
          }
        }
      },
