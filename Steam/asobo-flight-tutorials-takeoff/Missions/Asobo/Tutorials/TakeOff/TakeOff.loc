      "TakeOff.Dlg_UnexpectedLanding2": {
        "UUID": "a3e54695-3ff0-4a3d-9043-e909cfc5c82c",
        "Speaker": "INSTRUCTOR",
        "LastModifiedBy": "ajimeno",
        "LastModifiedDate": "2020-02-28 11:26:26",
        "Languages": {
          "en-US": {
            "Text": "It's well done, don't get me wrong… just not what was expected."
          },
          "fr-FR": {
            "Text": "Je ne vous jette pas la pierre, mais ce n'était pas prévu au programme..."
          },
          "es-ES": {
            "Text": "A ver, no me malinterpretes... Simplemente no es como estaba previsto."
          },
          "de-DE": {
            "Text": "Eine gute Landung, aber leider nicht das, was ich erwartet hatte."
          },
          "it-IT": {
            "Text": "Non fraintendermi, hai fatto un buon lavoro... ma non era quello che mi aspettavo."
          },
          "pl-PL": {
            "Text": "Nie zrozum mnie źle, poszło ci świetnie… ale nie o to chodziło."
          },
          "pt-BR": {
            "Text": "Foi bem feito, não me entenda mal… só não foi o que eu esperava."
          },
          "ru-RU": {
            "Text": "Поймите правильно… Посадка прошла успешно, но задача была не совсем такой."
          },
          "zh-CN": {
            "Text": "It's well done, don't get me wrong… just not what was expected."
          }
        }
      },
      "TakeOff.Dlg_UnexpectedLanding2_MALE": {
