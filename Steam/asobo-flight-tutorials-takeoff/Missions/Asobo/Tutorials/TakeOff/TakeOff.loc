      "TakeOff.Dlg_ApproachPath1": {
        "UUID": "268f688e-79f0-47ec-9c54-d0fa7631f109",
        "Speaker": "INSTRUCTOR",
        "LastModifiedBy": "ajimeno",
        "LastModifiedDate": "2020-02-17 13:54:56",
        "LocalizationStatus": "TranslationNeeded",
        "Tags": {
          "Locked": "False"
        },
        "Languages": {
          "en-US": {
            "Text": "看起来一切正常，跑道上无人穿行。现在将飞机滑行至起飞位置。",
            "LocalizationStatus": "TranslationNeeded"
          },
          "fr-FR": {
            "Text": "Tout semble en ordre, pas de circulation transversale. Procédez et roulez jusqu'à la position.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "es-ES": {
            "Text": "Parece que no hay tránsito. Adelante, ya puedes rodar hacia tu posición.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "de-DE": {
            "Text": "Sieht gut aus, kein Verkehr, der uns betreffen würde. Sie können jetzt zur Startposition rollen.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "it-IT": {
            "Text": "È tutto a posto, niente traffico incrociato. Procedi con il rullaggio per metterti in posizione.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "pl-PL": {
            "Text": "Wygląda na to, że wszystko w porządku, nie ma kolizyjnego ruchu. Kołuj na pozycję.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "pt-BR": {
            "Text": "Tudo parece estar bom, sem tráfego cruzado. Vá em frente e faça o taxiamento até a posição.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "ru-RU": {
            "Text": "Похоже, всё в порядке, перекрёстного движения нет. Выруливайте на позицию.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "zh-CN": {
            "Text": "看起来一切正常，跑道上无人穿行。现在将飞机滑行至起飞位置。",
            "LocalizationStatus": "TranslationNeeded"
          }
        }
      },
      "TakeOff.Dlg_ApproachPath1_MALE": {
        "UUID": "33bca9f8-9604-447d-8e7f-f10038b813bd",
        "Speaker": "INSTRUCTOR",
        "LastModifiedBy": "ajimeno",
        "LastModifiedDate": "2020-02-28 11:26:26",
        "Languages": {
          "en-US": {
            "Text": "Alright -- no cross-traffic to contend with. You're clear to taxi."
          },
          "fr-FR": {
            "Text": "Bien, pas de circulation transversale. On peut rouler."
          },
          "es-ES": {
            "Text": "Está bien, no hay tránsito adverso. Tienes vía libre para el rodaje."
          },
          "de-DE": {
            "Text": "Gut, keine anderen Flugzeuge, die uns gefährlich werden könnten. Ab aufs Rollfeld."
          },
          "it-IT": {
            "Text": "Bene, niente traffico incrociato. Puoi procedere con il rullaggio."
          },
          "pl-PL": {
            "Text": "W porządku, nie ma ruchu kolizyjnego. Możesz kołować."
          },
          "pt-BR": {
            "Text": "Certo. Nenhum tráfego cruzado à vista. Você está livre para taxiar."
          },
          "ru-RU": {
            "Text": "Отлично. Перекрёстного движения нет, можно выполнять руление."
          },
          "zh-CN": {
            "Text": "Alright -- no cross-traffic to contend with. You're clear to taxi."
          }
        }
      },
      "TakeOff.Dlg_ApproachPath2": {
