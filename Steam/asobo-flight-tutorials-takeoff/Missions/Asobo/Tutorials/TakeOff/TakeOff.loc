        },
        "Languages": {
          "en-US": {
            "Text": "在地面上时，可以使用方向舵控制航空器转弯。"
          },
          "fr-FR": {
            "Text": "Avec les pédales du palonnier, vous ne devriez avoir aucun mal à tourner."
          },
          "es-ES": {
            "Text": "Los pedales del timón de dirección facilitan el manejo del avión."
          },
          "de-DE": {
            "Text": "Mit den Seitenruderpedalen sollten Sie das Flugzeug ganz bequem steuern können."
          },
          "it-IT": {
            "Text": "I pedali del timone fanno virare facilmente l'aeromobile."
          },
          "pl-PL": {
            "Text": "Kierowanie za pomocą pedałów orczyka powinno być dość łatwe."
          },
          "pt-BR": {
            "Text": "Os pedais do leme direcional devem facilitar a manobra da aeronave."
          },
          "ru-RU": {
            "Text": "Педали руля направления упрощают управление самолётом на земле."
          },
          "zh-CN": {
            "Text": "在地面上时，可以使用方向舵控制航空器转弯。"
          }
        }
      },
      "TakeOff.Dlg_ApproachPath2_MALE": {
        "UUID": "c812dd44-bf70-4a04-88b9-2576bdb939b3",
        "Speaker": "INSTRUCTOR",
        "LastModifiedBy": "ajimeno",
        "LastModifiedDate": "2020-03-27 08:04:50",
        "Languages": {
          "en-US": {
            "Text": "Use your Rudder pedals to steer the aircraft into position."
          },
          "fr-FR": {
            "Text": "Utilisez les pédales du palonnier pour diriger l'avion jusqu'à la position."
          },
          "es-ES": {
            "Text": "Usa los pedales del timón de dirección para situar la aeronave en posición."
          },
          "de-DE": {
            "Text": "Bringen Sie das Flugzeug mit den Seitenruderpedalen in Position."
          },
          "it-IT": {
            "Text": "Utilizza i pedali del timone per portare l'aeromobile in posizione."
          },
          "pl-PL": {
            "Text": "Podczas kołowania na start kieruj samolotem, używając pedałów orczyka."
          },
          "pt-BR": {
            "Text": "Utilize os pedais do leme direcional para manobrar a aeronave até a posição correta."
          },
          "ru-RU": {
            "Text": "При помощи педалей руля направления выведите судно на позицию."
          },
          "zh-CN": {
            "Text": "Use your Rudder pedals to steer the aircraft into position."
          }
        }
      },
