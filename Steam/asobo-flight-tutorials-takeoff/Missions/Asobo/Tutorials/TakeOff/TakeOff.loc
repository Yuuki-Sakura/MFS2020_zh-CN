        "LastModifiedBy": "ajimeno",
        "LastModifiedDate": "2020-04-08 23:48:13",
        "Languages": {
          "en-US": {
            "Text": "It's not the end of the world, just unexpected."
          },
          "fr-FR": {
            "Text": "Rien de dramatique, mais ce n'était pas prévu."
          },
          "es-ES": {
            "Text": "No es el fin del mundo, pero estaba previsto de otro modo."
          },
          "de-DE": {
            "Text": "Das ist jetzt kein Weltuntergang, es kommt nur etwas unerwartet."
          },
          "it-IT": {
            "Text": "Non è la fine del mondo, solo un po' inaspettato."
          },
          "pl-PL": {
            "Text": "Świat się nie zawalił, to tylko nieoczekiwany problem."
          },
          "pt-BR": {
            "Text": "Não é o fim do mundo, só foi um pouco inesperado."
          },
          "ru-RU": {
            "Text": "Ничего страшного не случилось, но это было неожиданно."
          },
          "zh-CN": {
            "Text": "It's not the end of the world, just unexpected."
          }
        }
      },
