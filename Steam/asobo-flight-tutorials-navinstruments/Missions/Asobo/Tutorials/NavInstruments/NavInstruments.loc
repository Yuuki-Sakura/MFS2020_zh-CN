      "NavInstruments.Dlg_TakeOff1": {
        "UUID": "c62eb7f3-3b02-4b8c-88d1-989534c4857a",
        "Speaker": "INSTRUCTOR",
        "LastModifiedBy": "ajimeno",
        "LastModifiedDate": "2020-02-28 11:26:27",
        "Languages": {
          "en-US": {
            "Text": "Alright, we're off. Now, keep climbing around the airport til we reach 5,800 feet."
          },
