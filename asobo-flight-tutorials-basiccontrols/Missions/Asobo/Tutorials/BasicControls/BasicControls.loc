          "pl-PL": {
            "Text": "Popatrz przez boczne okna. Widzialność jest dziś świetna.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "pt-BR": {
            "Text": "Olhe pelas janelas laterais. A visibilidade está ótima hoje.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "ru-RU": {
            "Text": "Посмотрите в боковые окна. Сегодня отличная видимость.",
            "LocalizationStatus": "TranslationNeeded"
          }
        }
      },
