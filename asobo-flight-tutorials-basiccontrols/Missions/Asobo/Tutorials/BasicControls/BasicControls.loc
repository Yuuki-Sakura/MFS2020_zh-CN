      "BasicControls.Dlg_RollStraight2_1_MALE": {
        "UUID": "f94620db-9e06-450b-97f0-725480c9288f",
        "Speaker": "INSTRUCTOR",
        "LastModifiedBy": "ajimeno",
        "LastModifiedDate": "2020-03-18 12:01:41",
        "LocalizationStatus": "TranslationNeeded",
        "Languages": {
          "en-US": {
            "Text": "The Yoke also controls your aircraft's Elevator, pitching you down if you push forward on the stick, up if you pull back."
          },
          "fr-FR": {
            "Text": "Le manche actionne aussi la gouverne de profondeur. On pousse le manche pour piquer, et on tire sur le manche pour cabrer.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "es-ES": {
            "Text": "El volante también controla el timón de profundidad de la aeronave, reduciendo el cabeceo al empujarlo y aumentándolo al tirar de él.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "de-DE": {
            "Text": "Auch das Höhenruder steuern Sie mit dem Steuerhorn. Drücken Sie den Steuerknüppel nach vorne, geht die Nase runter – und andersherum.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "it-IT": {
            "Text": "La barra controlla anche l'equilibratore dell'aeromobile, se spingi la barra di comando esegui un beccheggio verso il basso, se la tiri, esegui un beccheggio verso l'alto.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "pl-PL": {
            "Text": "Wolant steruje również sterem wysokości, powodując pochylenie dziobu, gdy go odepchniesz, i uniesienie, gdy ściągniesz na siebie.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "pt-BR": {
            "Text": "O manche também controla o profundor da sua aeronave, arfando-a para baixo se você picar o manche para frente, ou para cima se você cabrar.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "ru-RU": {
            "Text": "Штурвал также управляет и рулём высоты. Если дать штурвал от себя, судно начнёт снижаться, если взять на себя — перейдёт в набор высоты.",
            "LocalizationStatus": "TranslationNeeded"
          }
        }
      },
      "BasicControls.Dlg_RollStraight2_MALE": {
        "UUID": "a3c2cffc-f52c-4b33-94d1-30c6f27cd299",
        "Speaker": "INSTRUCTOR",
        "LastModifiedBy": "ajimeno",
        "LastModifiedDate": "2020-04-15 18:16:25",
        "LocalizationStatus": "TranslationNeeded",
        "Languages": {
          "en-US": {
            "Text": "Now let's talk about Pitch."
          },
          "fr-FR": {
            "Text": "Parlons maintenant du tangage."
          },
          "es-ES": {
            "Text": "Ahora hablemos del cabeceo."
          },
          "de-DE": {
            "Text": "Als Nächstes: die Neigung."
          },
          "it-IT": {
            "Text": "Ora parliamo del beccheggio."
          },
          "pl-PL": {
            "Text": "Pomówmy teraz o pochyleniu."
          },
          "pt-BR": {
            "Text": "Agora vamos falar sobre arfagem."
          },
          "ru-RU": {
            "Text": "Давайте поговорим о тангаже."
          }
        }
      },
      "BasicControls.Dlg_RollStraight3": {
        "UUID": "8cad7113-8f39-4e02-a2d7-52a14f0435c8",
        "Speaker": "INSTRUCTOR",
        "LastModifiedBy": "ajimeno",
        "LastModifiedDate": "2020-03-18 12:01:41",
        "LocalizationStatus": "TranslationNeeded",
        "Tags": {
          "Locked": "False"
        },
        "Languages": {
          "en-US": {
            "Text": "回拉控制杆，让飞机开始爬升。",
            "LocalizationStatus": "TranslationNeeded"
          },
          "fr-FR": {
            "Text": "Tirez sur le manche pour que l'avion prenne de l'altitude. Allez-y.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "es-ES": {
            "Text": "Tira hacia atrás de la palanca de mando para que el avión ascienda. Venga, prueba.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "de-DE": {
            "Text": "Ziehen Sie das Horn nach hinten, um das Flugzeug aufsteigen zu lassen. Nur zu.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "it-IT": {
            "Text": "Tira leggermente la barra di comando e l'aeromobile comincerà a salire. Fai una prova.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "pl-PL": {
            "Text": "Ściągnij wolant na siebie, by zacząć się wznosić.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "pt-BR": {
            "Text": "Cabre o manche para que a aeronave comece a subir. Tente aí.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "ru-RU": {
            "Text": "Возьмите ручку управления на себя, и самолёт перейдёт в набор высоты. Попробуйте.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "zh-CN": {
            "Text": "回拉控制杆，让飞机开始爬升。",
            "LocalizationStatus": "TranslationNeeded"
          }
        }
      },
