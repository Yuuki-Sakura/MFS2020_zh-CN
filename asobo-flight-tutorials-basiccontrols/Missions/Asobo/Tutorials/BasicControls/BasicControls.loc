      "BasicControls.Dlg_Cameras": {
        "UUID": "a797b155-aec3-4c96-8b7f-c65b81da0d99",
        "Speaker": "INSTRUCTOR",
        "LastModifiedBy": "ajimeno",
        "LastModifiedDate": "2020-03-18 12:01:40",
        "LocalizationStatus": "TranslationNeeded",
        "Tags": {
          "Locked": "False"
        },
        "Languages": {
          "en-US": {
            "Text": "从侧面的窗户望出去，可以看到塞多纳地区今日能见度良好。",
            "LocalizationStatus": "TranslationNeeded"
          },
          "fr-FR": {
            "Text": "Vous pouvez voir, par vos fenêtres latérales, que la visibilité sur Sedona est bonne aujourd'hui.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "es-ES": {
            "Text": "Por las ventanas laterales podrás contemplar la magnífica visibilidad que hay hoy sobre Sedona.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "de-DE": {
            "Text": "Wenn Sie aus Ihren Seitenfenstern schauen, haben Sie heute einen wundervollen Blick auf die Umgebung von Sedona.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "it-IT": {
            "Text": "Fuori dai finestrini laterali puoi vedere che oggi c'è una visibilità buonissima su Sedona.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "pl-PL": {
            "Text": "Mamy dziś nad Sedoną świetną widzialność przez boczne okna.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "pt-BR": {
            "Text": "Pelas janelas laterais, você pode ver que temos excelente visibilidade sobre Sedona hoje.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "ru-RU": {
            "Text": "В боковые окна можно наблюдать, что сегодня в Седоне отличная видимость.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "zh-CN": {
            "Text": "从侧面的窗户望出去，可以看到塞多纳地区今日能见度良好。",
            "LocalizationStatus": "TranslationNeeded"
          }
        }
      },
      "BasicControls.Dlg_Cameras_MALE": {
        "UUID": "2814c318-9cbc-466c-a40c-adf5d73a549c",
        "Speaker": "INSTRUCTOR",
        "LastModifiedBy": "ajimeno",
        "LastModifiedDate": "2020-03-18 12:01:40",
