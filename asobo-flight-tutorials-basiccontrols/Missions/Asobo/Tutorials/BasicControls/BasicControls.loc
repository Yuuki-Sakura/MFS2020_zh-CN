          "Locked": "False"
        },
        "Languages": {
          "en-US": {
            "Text": "你快要飞出训练空域了，快回头！",
            "LocalizationStatus": "TranslationNeeded"
          },
          "fr-FR": {
            "Text": "Vous avez quitté l'espace aérien de formation, vous devez faire demi-tour.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "es-ES": {
            "Text": "Has abandonado el espacio aéreo de entrenamiento. Tienes que regresar.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "de-DE": {
            "Text": "Sie haben den Übungsluftraum verlassen, Sie müssen umkehren!",
            "LocalizationStatus": "TranslationNeeded"
          },
          "it-IT": {
            "Text": "Hai lasciato lo spazio aereo per l'addestramento, devi tornare indietro.",
