        "Languages": {
          "en-US": {
            "Text": "塞多纳机场在哪来着…",
            "LocalizationStatus": "TranslationNeeded"
          },
          "fr-FR": {
            "Text": "Vous apercevez l'aéroport de Sedona ? En principe, la piste n'est pas difficile à repérer.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "es-ES": {
            "Text": "A ver si puedes localizar el aeropuerto de Sedona. No debería de ser muy difícil ver la pista de aterrizaje.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "de-DE": {
            "Text": "Halten Sie jetzt bitte nach dem Flugplatz von Sedona Ausschau. Die Landebahn sollte leicht zu finden sein.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "it-IT": {
            "Text": "Prova a individuare l'Aeroporto di Sedona. La pista si dovrebbe riconoscere facilmente.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "pl-PL": {
            "Text": "Zobaczmy, czy wypatrzysz lotnisko Sedona. To nie powinno być trudne, jego pas jest dość charakterystyczny.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "pt-BR": {
            "Text": "Veja se consegue localizar o Aeroporto de Sedona. A pista deve ser uma referência fácil de encontrar.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "ru-RU": {
            "Text": "Попробуйте найти аэропорт Седона. Обычно полосу хорошо видно с воздуха.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "zh-CN": {
            "Text": "塞多纳机场在哪来着…",
            "LocalizationStatus": "TranslationNeeded"
          }
        }
      },
