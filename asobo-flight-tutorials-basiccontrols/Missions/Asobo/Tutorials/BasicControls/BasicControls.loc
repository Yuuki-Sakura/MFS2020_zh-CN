        "Speaker": "INSTRUCTOR",
        "LastModifiedBy": "ajimeno",
        "LastModifiedDate": "2020-03-18 12:01:40",
        "LocalizationStatus": "TranslationNeeded",
        "Languages": {
          "en-US": {
            "Text": "You've crossed out of the Training zone. Better get back in there."
          },
          "fr-FR": {
            "Text": "Vous avez dépassé la zone de formation. Il faut revenir.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "es-ES": {
            "Text": "Has abandonado la zona de entrenamiento. Será mejor que regreses.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "de-DE": {
            "Text": "Sie haben den Übungsluftraum verlassen. Kehren Sie bitte um.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "it-IT": {
            "Text": "Hai lasciato l'area di addestramento. Meglio tornare indietro.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "pl-PL": {
            "Text": "Jesteś poza strefą lotów szkolnych. Lepiej do niej powróć.",
