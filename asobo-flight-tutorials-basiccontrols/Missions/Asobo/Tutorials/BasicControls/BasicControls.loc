      "BasicControls.Dlg_CopilotControl": {
        "UUID": "34d0a1dc-2f4c-46f6-b542-ecac7232b236",
        "Speaker": "INSTRUCTOR",
        "LastModifiedBy": "ajimeno",
        "LastModifiedDate": "2020-02-17 13:54:57",
        "LocalizationStatus": "TranslationNeeded",
        "Tags": {
          "Locked": "False"
        },
        "Languages": {
          "en-US": {
            "Text": "不用担心，你熟悉环境的时候，由我负责控制飞机。",
            "LocalizationStatus": "TranslationNeeded"
          },
          "fr-FR": {
            "Text": "Prenez vos repères. Ne vous inquiétez pas, je tiens le manche !",
            "LocalizationStatus": "TranslationNeeded"
          },
          "es-ES": {
            "Text": "No te preocupes, yo sujeto la palanca de mando mientras te orientas.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "de-DE": {
            "Text": "Keine Sorge, ich steuere das Flugzeug, während Sie sich mit Ihrer Umgebung vertraut machen.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "it-IT": {
            "Text": "Non preoccuparti, io mi occupo della barra mentre tu ti orienti.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "pl-PL": {
            "Text": "Nie martw się, w razie czego potrzymam stery.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "pt-BR": {
            "Text": "Não se preocupe, eu cuido do manche enquanto você se orienta.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "ru-RU": {
            "Text": "Не волнуйтесь, я возьму управление на себя, пока вы осматриваетесь.",
            "LocalizationStatus": "TranslationNeeded"
          },
          "zh-CN": {
            "Text": "不用担心，你熟悉环境的时候，由我负责控制飞机。",
            "LocalizationStatus": "TranslationNeeded"
          }
        }
      },
